{
  "name": "weather-app",
  "version": "0.0.1",
  "description": "weather app",
  "repository": "",
  "main": "server/index.js",
  "author": "Kang",
  "license": "MIT",
  "scripts": {
    "start": "node server",
    "build": "NODE_ENV=production NODE_OPTIONS=--openssl-legacy-provider npm run webpack",
    "dev": "NODE_ENV=development run-p dev:client dev:server",
    "dev:client": "npm run webpack -- --watch",
    "dev:server": "nodemon server",
    "webpack": "webpack --config ./client/webpack.config.js",
    "test": "jest",
    "test:watch": "jest --watchAll --verbose",
    "lint": "eslint --ext .js,.jsx ."
  },
  "keywords": [
    "frontend",
    "backend"
  ],
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-transform-runtime"
    ]
  },
  "eslintConfig": {
    "extends": "react-app",
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error"
    }
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "bootstrap": "^5.2.3",
    "classnames": "^2.3.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "esri-leaflet": "^3.0.10",
    "esri-leaflet-geocoder": "^3.1.3",
    "esri-leaflet-vector": "^4.0.0",
    "express": "^4.16.2",
    "gridfs-stream": "^1.1.1",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "leaflet": "^1.9.3",
    "leaflet-geosearch": "^3.7.0",
    "lodash": "^4.17.21",
    "method-override": "^3.0.0",
    "mongoose": "^5.0.9",
    "react": "^18.2.0",
    "react-bootstrap": "^2.7.0",
    "react-leaflet": "^4.2.0",
    "react-papaparse": "^4.0.2",
    "react-refresh": "^0.14.0",
    "react-router-dom": "^6.2.1",
    "react-select": "^5.7.0",
    "react-toastify": "^9.1.1",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.1.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.13.1",
    "@babel/plugin-proposal-class-properties": "7.13.0",
    "@babel/plugin-proposal-object-rest-spread": "7.13.0",
    "@babel/plugin-transform-runtime": "7.13.1",
    "@babel/preset-env": "7.13.0",
    "@babel/preset-react": "7.12.13",
    "@testing-library/react": "11.2.5",
    "babel-loader": "^8.2.2",
    "css-loader": "^6.6.0",
    "eslint": "^8.31.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "2.8.2",
    "file-loader": "^6.2.0",
    "file-saver": "^2.0.5",
    "jest": "^26.6.3",
    "mini-css-extract-plugin": "^2.5.3",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "react-dom": "^18.2.0",
    "sass-loader": "^12.4.0",
    "style-loader": "^3.3.1",
    "superagent": "6.1.0",
    "supertest": "^6.1.3",
    "url-loader": "^4.1.1",
    "webpack": "5.24.0",
    "webpack-cli": "4.5.0"
  }
}
